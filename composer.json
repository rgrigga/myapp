{
    "name": "rgrigga/workbench",
    "description": "Laravel 4 Starter Site",
    "keywords": ["laravel","Starter Site"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Elkins",
            "homepage": "http://www.andrewelkins.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:dclem/av2.0"
        },
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:dclem/av2.0"
        },
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:rgrigga/blog"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:rgrigga/Laravel-4-Bootstrap-Starter-Site"
        }
    ],
	"require": {
		"laravel/framework": "4.1.*",
		"way/generators": "dev-master",
		"greggilbert/recaptcha": "dev-master",
		"zizaco/confide": "dev-master",
		"zizaco/entrust": "dev-master",
		"robclancy/presenter": "1.2.*",
		"jasonlewis/basset": "dev-master",
		"leafo/lessphp": "0.4.0"
	},
	"require-dev": {
		"andrew13/cabinet":"dev-master",
        "rgrigga/blog":"dev-master",
		"rgrigga/cabinet":"dev-master",
        "rgrigga/Laravel-4-Bootstrap-Starter-Site":"dev-master",
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master@dev",
        "summerstreet/woodling": "0.1.*",
        "barryvdh/laravel-ide-helper": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
            "app/library",
			"app/models",
			"app/presenters",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/library"
		]
	},

	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
